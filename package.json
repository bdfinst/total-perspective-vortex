{
  "name": "vsm-tool",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build && next export",
    "start": "next start",
    "test": "jest --collectCoverage",
    "ci": "jest --watch",
    "format:check": "prettier --list-different './src/**/*.js*' './src/**/*.css'",
    "format:fix": "prettier --write './src/**/*.js*' './src/**/*.css'",
    "lint:check": "eslint src",
    "lint:fix": "eslint src --fix",
    "lint": "npm run format:fix && npm run lint:fix",
    "depcheck": "npx depcheck"
  },
  "dependencies": {
    "@material-ui/core": "^4.11.3",
    "@material-ui/icons": "^4.11.2",
    "local-storage": "^2.0.0",
    "next": "10.0.6",
    "object-key-validator": "^1.1.2",
    "react": "^16.8.4",
    "react-dom": "^16.8.4",
    "react-flow-renderer": "^8.7.0"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^5.11.9",
    "@testing-library/react": "^9.5.0",
    "@testing-library/react-hooks": "^5.0.3",
    "babel-eslint": "^10.1.0",
    "babel-jest": "^26.6.3",
    "eslint": "^6.4.0",
    "eslint-config-prettier": "^6.9.0",
    "eslint-config-react-app": "^6.0.0",
    "eslint-plugin-flowtype": "^5.2.0",
    "eslint-plugin-import": "^2.20.0",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-prettier": "^3.1.2",
    "eslint-plugin-react": "^7.18.0",
    "eslint-plugin-react-hooks": "^4.2.0",
    "husky": "^4.2.0",
    "identity-obj-proxy": "^3.0.0",
    "import-sort-style-absolute": "^1.0.1",
    "jest": "^26.6.3",
    "prettier": "^1.19.1",
    "react-test-renderer": "^16.12.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint && git add . && npm run lint:check && npm run format:check && npm test"
    }
  },
  "importSort": {
    ".js, .jsx, .es6, .es": {
      "parser": "babylon",
      "style": "absolute"
    }
  }
}
