{
  "name": "vsm-tool",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "cypress": "npm run dev:wait cy:open",
    "cy:open": "cypress open",
    "dev": "next dev",
    "build": "next build && next export",
    "start": "next start",
    "test": "jest --collectCoverage",
    "test:e2e": "npm run dev:wait test:chrome",
    "test:chrome": "cypress run --browser chrome --headless",
    "dev:wait": "start-server-and-test dev http-get://localhost:3000",
    "ci": "jest --watch",
    "format:check": "prettier --list-different './src/**/*.js*' './src/**/*.css'",
    "format:fix": "prettier --write './src/**/*.js*' './src/**/*.css'",
    "lint:check": "eslint src",
    "lint:fix": "eslint src --fix",
    "lint": "npm run format:fix && npm run lint:fix"
  },
  "dependencies": {
    "@emotion/core": "^11.0.0",
    "@emotion/styled": "^11.1.5",
    "@material-ui/core": "^4.11.3",
    "@material-ui/icons": "^4.11.2",
    "@mrblenny/react-flow-chart": "^0.0.14",
    "@projectstorm/react-diagrams": "^6.0.1-beta.7",
    "closest": "^0.0.1",
    "dagre": "^0.8.5",
    "lodash": "^4.17.20",
    "next": "10.0.6",
    "object-key-validator": "^1.1.2",
    "pathfinding": "^0.4.18",
    "paths-js": "^0.4.11",
    "react": "^16.14.0",
    "react-dom": "^16.8.4",
    "react-flow-renderer": "^8.7.0",
    "resize-observer-polyfill": "^1.5.1"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.12.11",
    "@babel/preset-react": "^7.12.10",
    "@testing-library/dom": "^7.29.4",
    "@testing-library/jest-dom": "^5.11.9",
    "@testing-library/react": "^9.5.0",
    "@testing-library/react-hooks": "^5.0.3",
    "@testing-library/user-event": "^12.6.2",
    "babel-eslint": "^10.1.0",
    "babel-jest": "^26.6.3",
    "cypress": "^6.3.0",
    "eslint": "^6.4.0",
    "eslint-config-prettier": "^6.9.0",
    "eslint-config-react-app": "^6.0.0",
    "eslint-plugin-flowtype": "^5.2.0",
    "eslint-plugin-import": "^2.20.0",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-prettier": "^3.1.2",
    "eslint-plugin-react": "^7.18.0",
    "eslint-plugin-react-hooks": "^4.2.0",
    "file-coverage-report": "^1.1.0",
    "husky": "^4.2.0",
    "identity-obj-proxy": "^3.0.0",
    "import-sort-style-absolute": "^1.0.1",
    "jest": "^26.6.3",
    "jest-junit": "^10.0.0",
    "jest-localstorage-mock": "^2.4.0",
    "jest-sonar-reporter": "^2.0.0",
    "prettier": "^1.19.1",
    "react-test-renderer": "^16.12.0",
    "sinon": "^8.1.0",
    "start-server-and-test": "^1.12.0",
    "svelte-jester": "^1.3.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint && git add . && npm run lint:check && npm run format:check"
    }
  },
  "importSort": {
    ".js, .jsx, .es6, .es": {
      "parser": "babylon",
      "style": "absolute"
    }
  }
}
