{
  "name": "total-perspective-vortex",
  "version": "1.1.0",
  "description": "A tool for value stream mapping a process flow",
  "keywords": [
    "lean",
    "devops",
    "vsm"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/bdfinst/value-stream-map"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "format:check": "prettier --list-different './src/**/*.js*' './src/**/*.css'",
    "format:fix": "prettier --write './src/**/*.js*' './src/**/*.css'",
    "lint": "npm run format:fix && npm run lint:fix",
    "lint:check": "eslint src test",
    "lint:fix": "eslint src test --fix",
    "depcheck": "depcheck --ignores='import-sort-style-absolute'"
  },

  "dependencies": {
    "@material-ui/core": "^4.11.3",
    "@material-ui/icons": "^4.11.2",
    "clsx": "^1.1.1",
    "dagre": "^0.8.5",
    "export-from-json": "^1.3.4",
    "local-storage": "^2.0.0",
    "material-ui-dropzone": "^3.5.0",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-flow-renderer": "^9.0.4",
    "react-scripts": "4.0.3",
    "uuid": "^3.4.0",
    "web-vitals": "^1.0.1"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^5.11.9",
    "@testing-library/react": "^11.2.5",
    "@testing-library/react-hooks": "^5.0.3",
    "babel-eslint": "^10.1.0",
    "babel-jest": "^26.6.3",
    "depcheck": "^1.4.0",
    "eslint": "^7.20.0",
    "eslint-config-prettier": "^8.1.0",
    "eslint-config-react-app": "^6.0.0",
    "eslint-plugin-flowtype": "^5.3.1",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-prettier": "^3.3.1",
    "eslint-plugin-react": "^7.22.0",
    "eslint-plugin-react-hooks": "^4.2.0",
    "husky": "^5.1.1",
    "identity-obj-proxy": "^3.0.0",
    "import-sort-style-absolute": "^1.0.1",
    "jest": "^26.6.0",
    "object-key-validator": "^1.1.2",
    "prettier": "^2.2.1",
    "prop-types": "^15.7.2",
    "react-test-renderer": "^17.0.1"
  },
  "importSort": {
    ".js, .jsx, .es6, .es": {
      "parser": "babylon",
      "style": "absolute"
    }
  }
}
